<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:uni="https://uni-login.dk" xmlns:tns="https://uni-login.dk/data" elementFormDefault="qualified" targetNamespace="https://uni-login.dk/data" xml:lang="da" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="https://wsieksport.uni-login.dk:443/wsieksport-v4/ws?xsd=3" namespace="https://uni-login.dk" />
  <xs:annotation>
    <xs:documentation>Basisskema for import og eksport</xs:documentation>
  </xs:annotation>
  <xs:attributeGroup name="importSourceAttrs">
    <xs:annotation>
      <xs:documentation>
                Attributter, der identificerer en import.
                Anvendes i rodelementet i import og til oplistning af kilder i eksport.
            </xs:documentation>
    </xs:annotation>
    <xs:attribute name="sourceDateTime" type="xs:dateTime" use="required">
      <xs:annotation>
        <xs:documentation>
                    Tidspunkt for eksport af data fra kildesystemet.
                    Ved import skal tidspunktet være nyere end tidspunktet for sidste eksport, der er indlæst.
                </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="source" type="tns:source" use="required" />
    <xs:attribute name="schoolYear" use="required">
      <xs:simpleType>
        <xs:annotation>
          <xs:documentation>Skoleår angivet i formatet YYYY-YYYY, f.eks.: "2016-2017".</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:pattern value="2[0-9]{3}-2[0-9]{3}" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:complexType name="institutionBase">
    <xs:annotation>
      <xs:documentation>
                Basistype for institution uden grupper. Anvendes kun direkte i delete-skemaet,
                mens andre anvender institutionWithGroupsBase.
                Tilføjes: InstitutionPerson
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="InstitutionNumber" type="uni:Regnr">
        <xs:annotation>
          <xs:documentation>
                        Seks-tegns registreringsnummer, som identificerer en institution (kan indeholde både bogstaver og tal).
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="InstitutionName" type="uni:NonEmptyToken">
        <xs:annotation>
          <xs:documentation>Institutionsnavn. Kun information. Gemmes ikke i UNI•Login.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="institutionWithGroupsBase">
    <xs:annotation>
      <xs:documentation>
                Den egentlige basistype for institution, der anvendes i import og eksport.
                Tilføjes: InstitutionPerson (jf. institutionBase)
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tns:institutionBase">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="Group" type="tns:group">
            <xs:annotation>
              <xs:documentation>Grupper på institutionen.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="group">
    <xs:annotation>
      <xs:documentation>
                Grupper bruges til samling af personer på en institution. Både elever og lærere kan tilknyttes grupper.
                En gruppe kan f.eks. være klasser i grundskolen, hold, fag, studiegrupper eller lærerteams.
                Der er i gruppebegrebet ingen begrænsninger, i forhold til hvad den enkelte institution vil bruge det
                til.
                Grupper kan oprettes både eksplicit og implicit. Group-elementet bruges til eksplicit oprettelse af
                grupper i forbindelse med import og alle attributter på gruppen kan udfyldes.
                Hvis en gruppe refereres fra personer uden at den er oprettet med Group elementet, foretager UNI•Login
                en implicit oprettelse af gruppen med blot et GroupId.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="GroupId" type="uni:Gruppekode">
        <xs:annotation>
          <xs:documentation>
                        Gruppeid/holdID/fagkode som er unikt på institutionen. For eksempel "4a", "Tysk I" eller
                        "Dansklærere".
                        GroupId vises i brugerdministrationen og bruges ligeledes i forbindelse med tildeling af for
                        eksempel licenser.
                        Elever og lærere kan tilknyttes gruppen ved at angive MainGroupId eller GroupId på personen.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="GroupName" type="uni:Token100char">
        <xs:annotation>
          <xs:documentation>
                        Alias for GroupId, hvis man ønsker et supplerende navn til gruppen.
                        For eksempel, hvis 0b også kaldes ”Solsikken”, kan det angives her.
                        Hvis det er en implicit oprettelse sættes GroupName lig med GroupId.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GroupType" type="tns:importGroupType">
        <xs:annotation>
          <xs:documentation>Typen af gruppe der oprettes.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="GroupLevel" type="uni:Trin">
        <xs:annotation>
          <xs:documentation>
                        Stamklassens/hovedgruppens trin.
                        Skal angives for grupper med groupType "Hovedgruppe" og kun da.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Line" type="uni:Token40char">
        <xs:annotation>
          <xs:documentation>
                        Spor, linje, studieretning eller lignende for klassen/stamholdet.
                        For grundskoler f.eks. "B".
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="FromDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        Gruppens startdato.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ToDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>Gruppens slutdato.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="LocalPersonId" type="tns:localPersonId" />
  <xs:simpleType name="localPersonId">
    <xs:annotation>
      <xs:documentation>
                Den lokale identifikation af personen på institutionen.
                Identifikationen bruges til at genkende personer fra en import til den næste. Identifikationen skal
                derfor være unik inden for en import, således at InstitutionNumber+source+LocalPersonId skal være unik.
                Identifikationen skal være uforanderlig, således at en person, der én gang er importeret med et
                LocalPersonId, skal have det samme LocalPersonId i alle fremtidige importer.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="tns:singleToken">
      <xs:maxLength value="18" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="personMini">
    <xs:annotation>
      <xs:documentation>
                Minimal repræsentation af persondata. Anvendes direkte i lille eksport.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="tns:FirstName" />
      <xs:element ref="tns:FamilyName" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="personMedium">
    <xs:annotation>
      <xs:documentation>
                Mellemstor udgave af persondata. Anvendes direkte i medium eksport.
                Basistype for personFull; se nedenfor.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tns:personMini">
        <xs:sequence>
          <xs:element minOccurs="0" ref="tns:CivilRegistrationNumber">
            <xs:annotation>
              <xs:documentation>
                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                    Obligatorisk i import.
                                    Vises ikke i eksport for personer med navne- og adressebeskyttelse.
                                </xs:documentation>
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="tns:EmailAddress" />
          <xs:element minOccurs="0" ref="tns:BirthDate" />
          <xs:element minOccurs="0" ref="tns:Gender" />
          <xs:element minOccurs="0" ref="tns:PhotoId" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="personFullBase">
    <xs:annotation>
      <xs:documentation>
                Base for fuld udgave af persondata.
                Anvendes i import og fuld eksport.
                Tilføjes: HomePhoneNumber, WorkPhoneNumber, MobilePhoneNumber
                (De tre er ikke defineret ved hjælpe af elementreferencer, fordi de
                skal være nillable i eksport.)
                I importskemaet defineres nogle elementer som obligatoriske, mens
                de i eksport kan mangle pga. manglende data i ældre importer eller
                manuelt oprettede brugere.
                Desuden har import oplysninger om alias ved navnebeskyttelse.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tns:personMedium">
        <xs:sequence>
          <xs:element minOccurs="0" ref="tns:Address" />
        </xs:sequence>
        <xs:attribute name="protected" type="xs:boolean" use="required">
          <xs:annotation>
            <xs:documentation>
                            Har personen navne- og adressebeskyttelse?
                            Hvis true, skal AliasFirstName og AliasFamilyName være sat i import, og kun da.
                        </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="verificationLevel" type="uni:Verifikationsniveau" use="required">
          <xs:annotation>
            <xs:documentation>
                            Er informationen vedrørende personen verificeret i CPR-registret?
                            Er altid 0, hvis protected er true.
                        </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="FirstName" type="tns:name">
    <xs:annotation>
      <xs:documentation>Fornavne.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="FamilyName" type="tns:familyName">
    <xs:annotation>
      <xs:documentation>Efternavn.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CivilRegistrationNumber" type="tns:civilRegistrationNumber">
    <xs:annotation>
      <xs:documentation>Personnummer. Se dokumentation til type.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="EmailAddress" type="tns:email">
    <xs:annotation>
      <xs:documentation>E-mail.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="BirthDate" type="xs:date">
    <xs:annotation>
      <xs:documentation>Fødselsdato.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Gender" type="tns:gender">
    <xs:annotation>
      <xs:documentation>Personens køn.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PhotoId">
    <xs:annotation>
      <xs:documentation>Benyttes til at navngive foto med.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="uni:NonEmptyToken">
        <xs:maxLength value="30" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Address" type="tns:address">
    <xs:annotation>
      <xs:documentation>Adresse.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="address">
    <xs:annotation>
      <xs:documentation>Adresseoplysninger. Anvendes i person.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" ref="tns:StreetAddress" />
      <xs:element minOccurs="0" ref="tns:PostalCode" />
      <xs:element minOccurs="0" ref="tns:PostalDistrict" />
      <xs:element minOccurs="0" ref="tns:CountryCode" />
      <xs:element minOccurs="0" ref="tns:Country" />
      <xs:element minOccurs="0" ref="tns:MunicipalityCode" />
      <xs:element minOccurs="0" ref="tns:MunicipalityName" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="StreetAddress">
    <xs:annotation>
      <xs:documentation>Adressens vejnavn, nummer og etage.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="uni:NonEmptyToken">
        <xs:maxLength value="100" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="PostalCode">
    <xs:annotation>
      <xs:documentation>Postnummer.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="uni:NonEmptyToken">
        <xs:maxLength value="20" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="PostalDistrict">
    <xs:annotation>
      <xs:documentation>By.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="uni:NonEmptyToken">
        <xs:maxLength value="100" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="CountryCode" type="tns:countryCode">
    <xs:annotation>
      <xs:documentation>
                Adressens landekode efter ISO-3166-1 alpha-2.
                Danmark er "DK", Grønland er "GL", og Færøerne er "FO".
            </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Country">
    <xs:annotation>
      <xs:documentation>Adressens land.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="uni:NonEmptyToken">
        <xs:maxLength value="30" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="MunicipalityCode">
    <xs:annotation>
      <xs:documentation>Kommunekode.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="uni:NonEmptyToken">
        <xs:maxLength value="6" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="MunicipalityName" type="uni:Token75char">
    <xs:annotation>
      <xs:documentation>Kommunenavn.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="phoneNumberProtectable">
    <xs:annotation>
      <xs:documentation>
                Telefonnummer med oplysning om, hvorvidt telefonnummeret er beskyttet.
                Hvis telefonnummeret er beskyttet, er kun attributten sat, mens elementet
                er tomt.
                Anvendes i fuld eksport. Se til phoneNumberWithProtectionInfo for import.
            </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="tns:phoneNumberOrEmpty">
        <xs:attribute name="protected" type="xs:boolean" use="required">
          <xs:annotation>
            <xs:documentation>Er telefonnummeret beskyttet?</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="phoneNumberWithProtectionInfo">
    <xs:annotation>
      <xs:documentation>
                Telefonnummer med oplysning om, hvorvidt telefonnummeret er beskyttet.
                Anvendes i import.
            </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:restriction base="tns:phoneNumberProtectable">
        <xs:minLength value="1" />
      </xs:restriction>
    </xs:simpleContent>
  </xs:complexType>
  <xs:element name="Employee" type="tns:employee">
    <xs:annotation>
      <xs:documentation>Læreroplysninger om personen.</xs:documentation>
    </xs:annotation>
    <xs:unique name="unik_Ansatrolle_pr_ansat">
      <xs:selector xpath="tns:Role" />
      <xs:field xpath="." />
    </xs:unique>
  </xs:element>
  <xs:complexType name="employeeBase">
    <xs:annotation>
      <xs:documentation>
                Basistype for læreroplysninger.
                Der anvendes nedarvning for at undgå at udskille Type som element
                for at implementere overgangsskemaet.
            </xs:documentation>
    </xs:annotation>
  </xs:complexType>
  <xs:complexType name="employee">
    <xs:annotation>
      <xs:documentation>Læreroplysninger. Anvendes i institutionPerson.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tns:employeeBase">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="Role" type="uni:Ansatrolle" />
          <xs:element minOccurs="0" ref="tns:ShortName" />
          <xs:element minOccurs="0" ref="tns:Occupation" />
          <xs:element minOccurs="0" ref="tns:Location" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="tns:GroupId">
            <xs:annotation>
              <xs:documentation>De grupper, som medarbejderen er tilknyttet.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="ShortName">
    <xs:annotation>
      <xs:documentation>Initialer.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="uni:NonEmptyToken">
        <xs:maxLength value="20" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Occupation">
    <xs:annotation>
      <xs:documentation>Stilling.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="uni:NonEmptyToken">
        <xs:maxLength value="60" />
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:complexType name="studentMini">
    <xs:annotation>
      <xs:documentation>
                Minimal basistype for student.
                Elevoplysninger. Anvendes i institutionPerson.
                I import og fuld eksport tilføjes ContactPerson.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Role" type="uni:Elevrolle">
        <xs:annotation>
          <xs:documentation>Hvilke rolle har eleven.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="StudentNumber">
        <xs:annotation>
          <xs:documentation>Elevens studienummer fra lokalt administrativt system.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="uni:NonEmptyToken">
            <xs:maxLength value="26" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Level" type="uni:Trin">
        <xs:annotation>
          <xs:documentation>Elevens trin.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" ref="tns:Location" />
      <xs:element name="MainGroupId" type="uni:Gruppekode">
        <xs:annotation>
          <xs:documentation>
                        Elevens hovedgruppe.
                        For grundskoler er hovedgruppen elevens klasse, og for andre
                        institutioner kan den for eksempel være studieretning eller stamhold.
                        Feltet skal indeholde gruppens unikke id på institutionen (GroupId)
                        og må kun referere til grupper med groupType "Hovedgruppe".
                        Bemærk, at dette krav ikke kan garanteres at være overholdt i eksporten
                        pga. gamle importer og manuelt tilknyttede brugere.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="tns:GroupId">
        <xs:annotation>
          <xs:documentation>
                        Yderligere grupper, som eleven er tilknyttet, ud over tilknytningen via MainGroupId.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Location" type="uni:Token75char">
    <xs:annotation>
      <xs:documentation>Afdeling, bygning eller værelsesnummer på efterskoler.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="GroupId" type="uni:Gruppekode">
    <xs:annotation>
      <xs:documentation>
                Feltet skal indeholde gruppens unikke id på institutionen (GroupId).
            </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="contactPersonBase">
    <xs:annotation>
      <xs:documentation>
                Basistype for contactPerson. Anvendes i student i import og fuld eksport.
                Oplysninger om kontaktperson.
                Tilføjes Person, samt UNILogin i eksport.
            </xs:documentation>
    </xs:annotation>
    <xs:attribute name="relation" type="tns:contactPersonRelation" use="required">
      <xs:annotation>
        <xs:documentation>Kontaktpersonens relation til eleven.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="childCustody" type="xs:boolean" use="required">
      <xs:annotation>
        <xs:documentation>Har personen forældremyndighed?</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="accessLevel" type="uni:KontaktpersonAdgangsniveau">
      <xs:annotation>
        <xs:documentation>
                    Niveau for adgang til personoplysninger om den tilknyttede elev.
                    NB! Hvis kontaktpersonen ikke er forældremyndighedsindehaver, skal hjemmel opnås
                    på anden vis.
                    Attributten er valgfri i import. Hvis udeladt, regnes værdien implicit for 0 ved
                    childCustody=false og 1 ved childCustody=true. I eksport vil værdien altid være
                    angivet, uanset om den var implicit eller eksplicit i importen
                </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="Extern" type="tns:extern">
    <xs:annotation>
      <xs:documentation>Oplysninger om ekstern person.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="extern">
    <xs:annotation>
      <xs:documentation>
                Type til en ekstern person. Anvendes i InstitutionPerson i import og eksport.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Role" type="uni:Eksternrolle" />
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="tns:GroupId">
        <xs:annotation>
          <xs:documentation>De grupper, som den eksterne person er tilknyttet.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="uniLoginMini">
    <xs:annotation>
      <xs:documentation>
                Minimal basistype for UNI•Login-oplysninger.
                Anvendes direkte i lille eksport.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="tns:UserId" />
      <xs:element minOccurs="0" ref="tns:Name" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="uniLoginFull">
    <xs:annotation>
      <xs:documentation>
                Type for UNI•Login-oplysninger i fuld og medium eksport og returværdi i import.
                I lille eksport anvendes den minimale type.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tns:uniLoginMini">
        <xs:sequence>
          <xs:element ref="tns:InitialPassword" />
          <xs:element ref="tns:CivilRegistrationNumber" />
          <xs:element ref="tns:PasswordState" />
          <xs:element ref="tns:UniqueName" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="UserId" type="tns:singleToken">
    <xs:annotation>
      <xs:documentation>
                Unik UNI•Login-brugerid.
            </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Name" type="uni:NonEmptyToken">
    <xs:annotation>
      <xs:documentation>
                Brugerens navn i UNI•Login, f.eks.: "Peter Hansen".
